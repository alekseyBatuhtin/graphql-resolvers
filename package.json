{
  "name": "graphql-resolvers",
  "version": "0.3.3",
  "description": "Resolver composition library for GraphQL.",
  "keywords": [
    "apollo",
    "composition",
    "functional",
    "graphql",
    "resolver"
  ],
  "homepage": "https://github.com/lucasconstantino/graphql-resolvers#readme",
  "bugs": {
    "url": "https://github.com/lucasconstantino/graphql-resolvers/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucasconstantino/graphql-resolvers.git"
  },
  "license": "MIT",
  "author": "Lucas Constantino Silva <lucasconstantinosilva@gmail.com>",
  "main": "index.js",
  "scripts": {
    "codecov": "yarn coverage && codecov",
    "compile": "babel src -d lib",
    "coverage": "istanbul cover _mocha -- --require @babel/register --require @babel/polyfill",
    "lint": "eslint index.js src test",
    "qa": "yarn test && yarn lint",
    "test": "_mocha --require @babel/register --require @babel/polyfill"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "git add"
    ],
    "package.json": [
      "sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "deep-equal": "^1.0.1",
    "object-path": "^0.11.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-taller": "^0.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-spies": "^0.7.1",
    "codecov": "^2.1.0",
    "eslint": "^3.10.2",
    "eslint-config-taller": "^1.0.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "graphql": "^0.9.2",
    "graphql-tools": "^0.11.0",
    "husky": "^2.3.0",
    "istanbul": "^1.1.0-alpha.1",
    "lint-staged": "^8.1.7",
    "mocha": "^3.1.2",
    "prettier-eslint-cli": "^4.7.1",
    "ramda": "^0.23.0",
    "sort-package-json": "^1.22.1"
  }
}
